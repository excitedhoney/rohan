CREATE TABLE shbg (id INTEGER PRIMARY KEY AUTOINCREMENT, dt DATE DEFAULT (DATE()), sjzz TEXT NOT NULL, ltsz TEXT NOT NULL, cjl TEXT NOT NULL, cjje TEXT NOT NULL, cjbs TEXT NOT NULL, pjsyl TEXT NOT NULL, hsl TEXT NOT NULL);
